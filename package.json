{
  "name": "ymmp",
  "version": "1.0.1",
  "description": "Command-line interface for patching Yandex Music",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ymmp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && bun build ./src/index.ts --compile --minify --outfile ymmp.exe",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts"
  },
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.0.1",
    "@types/prompts": "^2.4.9",
    "@types/unzipper": "^0.10.11",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@electron/asar": "^4.0.1",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "ora": "^9.0.0",
    "prompts": "^2.4.2",
    "unzipper": "^0.12.3",
    "yaml": "^2.8.2"
  }
}
